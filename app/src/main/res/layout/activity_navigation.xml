<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/linearLayout1"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent" android:layout_height="match_parent"
	android:orientation="vertical" android:background="@drawable/basicbackground">
	<LinearLayout android:layout_height="40dip"
		android:layout_width="match_parent" android:orientation="horizontal"
		android:layout_gravity="center" android:baselineAligned="true"
		android:background="@drawable/ic_tab_template">
		<Spinner android:id="@+id/spinnerFromRoom"
			android:layout_width="160dip" android:layout_height="40dip"></Spinner>
		<Spinner android:id="@+id/spinnerToRoom" android:layout_width="160dip"
			android:layout_height="40dip"></Spinner>

	</LinearLayout>

</LinearLayout>
